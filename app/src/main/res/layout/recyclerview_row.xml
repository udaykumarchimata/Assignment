<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/firstTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/secondTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintEnd_toStartOf="@+id/codeTextView"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/thirdTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/firstTextView" />


    <TextView
        android:id="@+id/nameTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="name"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/thirdTextView"
        app:layout_constraintTop_toBottomOf="@+id/firstTextView" />

    <TextView
        android:id="@+id/regionTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="region"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/nameTextView"
        app:layout_constraintTop_toBottomOf="@+id/firstTextView" />

    <TextView
        android:id="@+id/codeTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="code"
        app:layout_constraintEnd_toStartOf="@+id/fourthTextView"
        app:layout_constraintTop_toBottomOf="@+id/secondTextView" />

    <TextView
        android:id="@+id/fourthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/secondTextView" />


    <TextView
        android:id="@+id/fifthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/thirdTextView" />

    <TextView
        android:id="@+id/sixthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintEnd_toStartOf="@+id/codeTextView"
        app:layout_constraintTop_toBottomOf="@+id/fourthTextView" />


    <TextView
        android:id="@+id/seventhTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/fifthTextView" />

    <TextView
        android:id="@+id/capitalTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="capital"
        app:layout_constraintStart_toEndOf="@+id/seventhTextView"
        app:layout_constraintTop_toBottomOf="@+id/fifthTextView" />

    <TextView
        android:id="@+id/eigthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toEndOf="@+id/sixthTextView"
        app:layout_constraintTop_toBottomOf="@+id/fifthTextView" />


    <TextView
        android:id="@+id/ninthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/seventhTextView" />

    <TextView
        android:id="@+id/tenthTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        android:text="|"
        app:layout_constraintEnd_toStartOf="@+id/codeTextView"
        app:layout_constraintTop_toBottomOf="@+id/seventhTextView" />

</androidx.constraintlayout.widget.ConstraintLayout>